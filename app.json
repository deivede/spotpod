{
  "expo": {
    "name": "spotpod",
    "slug": "spotpod",
    "version": "1.1.1",
    "scheme": "exp",
    "orientation": "portrait",
    "icon": "./spotpodlogo.png",
    "splash": {
      "image": "./splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
      },
      "adaptiveIcon": {
        "foregroundImage": "./spotpodlogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.opensource.spotpod",
      "permissions": []
    },
    "web": {
      "favicon": "./spotpodlogo.png"
    }
  }
}
